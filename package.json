    "defu": "^6.1.4",
    "hookable": "^5.5.3",
    "j2u": "^1.7.4",
    "mongodb": "^5.6.0",
    "ufo": "^1.5.3"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.3.13",
    "@nuxt/module-builder": "^0.6.0",
    "@nuxt/schema": "^3.11.2",
    "eslint": "^9.5.0",
    "nuxt": "^3.11.2",
    "typescript": "^5.4.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Morgbn/nuxt-oa.git"
